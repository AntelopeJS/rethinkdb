{
  "name": "@antelopejs/rethinkdb",
  "version": "0.1.1",
  "description": "RethinkDB module that implements the Database interface of antelopejs",
  "keywords": [
    "antelopejs",
    "rethinkdb",
    "database",
    "nosql",
    "realtime"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AntelopeJS/rethinkdb.git"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "Apache-2.0",
  "packageManager": "pnpm@10.6.5",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "dev:prepare": "cd playground && ajs module imports install",
    "dev": "ajs project run -w -p playground",
    "format": "prettier --write .",
    "generate": "ajs module exports generate",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prepack": "pnpm run build",
    "prepare": "ajs module imports install",
    "release": "pnpm run lint && pnpm run test && pnpm run prepack && release-it",
    "test": "echo \"No test specified\"",
    "changelog": "changelogen"
  },
  "antelopeJs": {
    "exportsPath": "dist/interfaces",
    "imports": [
      "core@beta",
      "database@beta"
    ],
    "importsOptional": [],
    "defaultConfig": {
      "pool": {
        "pool": true,
        "cursor": false,
        "silent": true,
        "user": "admin",
        "password": "",
        "discovery": true,
        "host": "127.0.0.1",
        "port": "28015"
      }
    }
  },
  "dependencies": {
    "reflect-metadata": "^0.2.2",
    "rethinkdb-ts": "^2.7.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.25.0",
    "@types/node": "^22.14.1",
    "changelogen": "^0.6.2",
    "eslint": "^9.25.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.6",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "release-it": "^19.0.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1"
  }
}
